{
  "name": "cli",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "browser": {
    "crypto": false
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.5",
    "aplayer": "^1.10.1",
    "axios": "^0.27.2",
    "core-js": "^3.8.3",
    "crypto-browserify": "^3.12.0",
    "el-table-infinite-scroll": "^2.0.0",
    "electron": "^19.0.10",
    "element-china-area-data": "^5.0.2",
    "element-plus": "^2.2.5",
    "hls.js": "^1.1.5",
    "js-file-download": "^0.4.12",
    "jwt-decode": "^3.1.2",
    "vue": "^3.2.13",
    "vue-router": "^4.0.16",
    "vue3-aplayer": "^1.7.3",
    "vuemoji-picker": "^0.1.4",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.31.0",
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "example/custom": true
    },
    "plugins": [
      "example"
    ],
    "extends": [
      "plugin:vue/vue3-essential"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-console": "off",
      "no-unused-vars": "off",
      "no-debugger": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
